package ${packageName};

import ${basePackage}.common.security.annotation.EnableCustomConfig;
import ${basePackage}.common.security.annotation.EnableRyFeignClients;
import ${basePackage}.common.swagger.annotation.EnableCustomSwagger2;
import org.springframework.boot.Banner;
import org.springframework.boot.SpringApplication;
import org.springframework.cloud.client.SpringCloudApplication;
/**
 * 代码生成
 *
 * @author css
 */
@EnableCustomConfig
@EnableCustomSwagger2
@EnableRyFeignClients
@SpringCloudApplication
public class ${BusinessName}Application {
    public static void main(String[] args) {
        SpringApplication app = new SpringApplication(${BusinessName}Application.class);
        app.setBannerMode(Banner.Mode.CONSOLE);
        app.run(args);
    }
}
