            <if test="departureDriverId != null "> and departure_driver_id = #{departureDriverId}</if>
    
            <if test="departureDriverName != null  and departureDriverName != ''"> and departure_driver_name like concat('%', #{departureDriverName}, '%')</if>
            <if test="departureVehicleId != null "> and departure_vehicle_id = #{departureVehicleId}</if>
    
