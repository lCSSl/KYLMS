            <if test="departureVehiclePlate != null  and departureVehiclePlate != ''"> and departure_vehicle_plate like concat('%', #{departureVehiclePlate}, '%')</if>
            <if test="departureTime != null "> and departure_time = #{departureTime}</if>
    
            <if test="status != null  and status != ''"> and status = #{status}</if>
    
        </where>
    </select>
    
