            <if test="deptId != null "> and dept_id = #{deptId}</if>
    
            <if test="stowageCode != null  and stowageCode != ''"> and stowage_code like concat('%', #{stowageCode}, '%')</if>
            <if test="stowageName != null "> and stowage_name like concat('%', #{stowageName}, '%')</if>
            <if test="stowageDestinationNode != null  and stowageDestinationNode != ''"> and stowage_destination_node = #{stowageDestinationNode}</if>
    
